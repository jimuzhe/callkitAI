# 开发环境专用配置, 启动命令: flutter run --dart-define=APP_ENV=dev
# 将此文件复制为 .env.dev 并填入真实值

# 小智实时对话服务
XIAOZHI_WS_URL=wss://dev.example.com/realtime
XIAOZHI_ACCESS_TOKEN=dev-token-placeholder
XIAOZHI_DEVICE_ID=dev-device-id
XIAOZHI_CLIENT_ID=dev-client-id
# 设置为 false 可强制使用 JSON 音频帧, 方便抓包
XIAOZHI_AUDIO_BINARY=false

# 和风天气项目配置 (可选覆盖)
WEATHER_API_HOST=dev.example.com
WEATHER_API_KEY=dev-weather-key
